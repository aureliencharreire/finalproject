<p id="notice"><%= notice %></p>

<h1>Listing Lots</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Type</th>
      <th>Typo</th>
      <th>Surface</th>
      <th>Parking</th>
      <th>Cave</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lots.each do |lot| %>
      <tr>
        <td><%= lot.number %></td>
        <td><%= lot.type %></td>
        <td><%= lot.typo %></td>
        <td><%= lot.surface %></td>
        <td><%= lot.parking %></td>
        <td><%= lot.cave %></td>
        <td><%= link_to 'Show', lot %></td>
        <td><%= link_to 'Edit', edit_lot_path(lot) %></td>
        <td><%= link_to 'Destroy', lot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lot', new_lot_path %>
